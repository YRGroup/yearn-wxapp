/**app.wxss**/
page {
  font-size: 32rpx;
  line-height:48rpx;
  color: #333;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.title {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
  text-align: center;
  font-size: 42rpx;
  padding: 40rpx 0;
}

.font-size-16 {
  font-size: 30rpx;
}

.font-size-18 {
  font-size: 34rpx;
}

.font-size-20 {
  font-size: 38rpx;
}

.background-cover {
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
}

.flex-column {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  flex-wrap: nowrap;
}

.flex-row {
  display: flex;
  justify-content: space-around;
}